mixin range-slider(skills, level)
	+b.range-slider&attributes(attributes)
		+e.INPUT.input(type='range', min='0', max='100', step='0.001', value=level)
		+e.UL.list-mobile
			-for (var i = 0; i < skills.length; ++i) {
			+e.LI.list-mobile-item= i+1
			-}
		+e.UL.list
			each item in skills
				+e.LI.list-item(data=item.position, style=`left:${item.position}%`)
					=item.title
